package java_array;

import java.util.Arrays;

public class C133 {
	public static void main(String[] args) {
		int[] arr = {10,20,30,400,500,110,540,900,200,23};
		System.out.println(Arrays.toString(arr));
		int mid = arr.length /2;
		for(int i = mid+1; i<arr.length-1; i++) {
			arr[i] = arr[i+1];
		}
		//arr[mid] = 0;
		System.out.println(Arrays.toString(arr));	
	}
}
