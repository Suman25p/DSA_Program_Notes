package java_array;

import java.util.Arrays;

public class C139 {
	public static void main(String[] args) {
		int[] arr = {10,20,30,400,500,110,540,900,200,23};
		System.out.println(Arrays.toString(arr));
		for(int i = arr.length -1; i>0; i--) {
			arr[i] = arr[i-1];
		}
		arr[0]= arr[arr.length-1];
		
		System.out.println(Arrays.toString(arr));	
	}
}
