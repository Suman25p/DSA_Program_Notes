package java_array;

import java.util.Arrays;

public class C136 {
	public static void main(String[] args) {
		int[] arr = {10,20,30,400,500,110,540,900,200,23};
		System.out.println(Arrays.toString(arr));
		int first = arr[0];
		for(int i = 0; i<arr.length-1; i++) {
			arr[i] = arr[i+1];
		}
		arr[arr.length] = first;
		System.out.println(Arrays.toString(arr));	
	}
}
