package java_array;

import java.util.Arrays;

public class C132 {
	public static void main(String[] args) {
		int[] arr = {10,20,30,400,500,110,540,900,200,23};
		System.out.println(Arrays.toString(arr));
		int mid = arr.length /2;
		for(int i = 0; i<mid -1; i++) {
			arr[i] = arr[i+1];
		}
		arr[arr.length-3] = 0;
		arr[arr.length-2] = 0;
		arr[arr.length-1] = 0;
		System.out.println(Arrays.toString(arr));	
	}
}
